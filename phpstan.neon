parameters:
	bootstrap: tests/bootstrap.php
	ignoreErrors:
	    - '#Call to an undefined static method DevboardLib\\GitHub\\Account\\AccountType::#'
	    - '#Call to an undefined static method DevboardLib\\GitHub\\PullRequest\\PullRequestAuthorAssociation::#'
	    - '#Call to an undefined static method DevboardLib\\GitHub\\PullRequestReview\\PullRequestReviewAuthorAssociation::#'
	    - '#Parameter .* Mockery\\MockInterface given.#'
	    - '#Parameter \#1 \$json of function json_decode expects string, string|false given.#'
	    - '#Method .*At::createFromFormat\(\) has parameter .* with no typehint specified.#'
	    - '#Method .*At::deserialize\(\) has parameter \$value with no typehint specified.#'
	    - '#Method .*MilestoneDueOn::createFromFormat\(\) has parameter .* with no typehint specified.#'
	    - '#Method .*MilestoneDueOn::deserialize\(\) has parameter \$value with no typehint specified.#'
includes:
	- vendor/phpstan/phpstan-phpunit/extension.neon
	- vendor/phpstan/phpstan-phpunit/rules.neon
	- vendor/phpstan/phpstan-strict-rules/rules.neon




